{
	"name": "w7/rangine-crontab",
	"description": "rangine crontab",
	"type": "library",
	"keywords": [
		"rangine",
		"crontab"
	],
	"license": "Apache-2.0",
	"require": {
		"php": ">=7.1.0",
		"w7/rangine": "*",
		"dragonmantank/cron-expression": "^2.2"
	},
	"require-dev": {
		"w7/php-cs-fixer": "^1.0",
		"phpunit/phpunit": "^8.0"
	},
	"autoload": {
		"psr-4": {
			"W7\\Crontab\\Tests\\" : "tests/",
			"W7\\Crontab\\": "src/"
		}
	},
	"extra": {
		"rangine": {
			"providers": [
				"W7\\Crontab\\ServiceProvider"
			]
		}
	}
}
